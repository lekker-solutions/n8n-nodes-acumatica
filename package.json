    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "gulp": "^4.0.2",
    "n8n": "^1.120.4",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.4"
  }
